//                                       ╰─────╯ │󰼓 ┋ 󱁐│ ╭─────╮    ╭─────╮ │󰭜 ┋ 󰼖│ ╰─────╯
//                                               ╰─────╯ │󰼔 ┋ 󰌒│    │⏎ ┋ 󰼕│ ╰─────╯
//                                                       ╰─────╯    ╰─────╯

        qmac1 {
            display-name = "qmac1";
            bindings = <
                &kp Q         &kp W        &kp E         &kp R           &kp T           &kp Y         &kp U          &kp I         &kp O        &kp P
                &hml LCTRL A  &hml LALT S  &hml LCMD D   &hml LSHFT F    &hml GLOBE G    &hmr GLOBE H  &hmr RSHFT J   &hmr RCMD K   &hmr RALT L  &hmr RCTRL APOS
         &to 3  &kp Z         &kp X        &kp C         &kp V           &kp B           &kp N         &kp M          &comma_semi   &dot_colon   &qu_excl         &tog 1
                                           &lt maus4 ESC  &lt arro5 SPACE  &lt calc6 TAB    &lt nums7 RET  &lt punc8 BSPC  &lt fkey9 DEL
            >;
        };

